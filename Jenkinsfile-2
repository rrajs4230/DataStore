@Library('my-shared-library') _
pipeline {
  agent any
  environment {
    DOCKERHUB_CREDENTIALS=credentials('dockerhub')
  }

  stages {
    stage("Clone Repository") {
      steps {
        gitCheckout(
          branch: "master",
          url: "https://github.com/shivakumar26011999/DataStore.git"
        )
      }
    }
  }
}
